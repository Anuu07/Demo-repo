   "source": [
    "def insertion_sort(a,n):\n",
    "    for i in range(1,n):\n",
    "        t=a[i]\n",
    "        k=i-1\n",
    "        while k>=0 and t<a[k]:\n",
    "            a[k+1] = a[k]\n",
    "            k -= 1\n",
    "        a[k + 1] = t\n",
    "a=[9,5,1,4,3]\n",
    "n=len(a)\n",
    "\n",
    "insertion_sort(a,n)\n",
    "print(\"Sorted array:\",a)"
   ]
  },
